# .bash_profile
# Get the aliases and functions
if [ -f ~/.bashrc ]; then
	. ~/.bashrc
fi

# User specific environment and startup programs
export PATH=$HOME/usr/bin:$HOME/usr/local/bin:/opt/local/bin/:$PATH
export LSCOLORS='exgxfxfxcxdxdxhbadbxbx'
export PERL5LIB=/Users/ignitus/usr/lib/perl5/site_perl/


